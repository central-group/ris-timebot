{
  "name": "ris-timebot",
  "version": "2.2.1",
  "description": "Bot approve time sheet automatic.",
  "main": "dist/main.js",
  "scripts": {
    "test": "mocha",
    "dev": "env-cmd .env backpack dev",
    "build": "backpack build && npm run pack",
    "pack": "pkg package.json --icon app.ico --output dist/ris-timebot.exe",
    "eslint": "eslint --ext .js --ignore-path .gitignore ."
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-preset-latest": "^6.24.1",
    "backpack-core": "^0.8.4",
    "env-cmd": "^8.0.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "bin": "dist/main.js",
  "pkg": {
    "icon": "app.ico",
    "targets": [
      "node10"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvgamerr/ris-timesheet-bot.git"
